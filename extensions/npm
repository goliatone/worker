#!/usr/bin/env bash

modules=(
  commitizen
  eslint
  mklicense
  mocha
  nativefier
  node-gyp
  nodemon
  npm
  speed-test
  webpack
  webpack-validator
  yo
)

generators=(
  generator-devine-project
  generator-eslint-config
  generator-generator
)

npm install -g ${modules[@]}
npm install -g ${generators[@]}
